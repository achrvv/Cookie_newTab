{
  "manifest_version" : 2,
  "name": "cookies",
  "version": "1.0",
  "description": "cookies_newTab",
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "newPage"
  },
  "chrome_url_overrides" :{
    "newtab": "index.html"
  },
  "background":{
    "scripts":["background.js"],
    "persistent": false
  }
}